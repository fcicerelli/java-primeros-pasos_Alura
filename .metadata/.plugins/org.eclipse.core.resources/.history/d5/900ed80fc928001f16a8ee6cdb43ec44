package javaPrimerosPasos;

public class Factorial {
	public static void main(String[] args) {
		for (int i = 1; i <= 10; i++) {
			int factorial = 0;
			for (int j = i; j <= 0; j--) {
				factorial = factorial + factorial * j;
			}
			System.out.println("El factorial de " + i + " es " + factorial);
		}
	}
}
